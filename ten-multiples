public class Week2 {
	
	static int[][] multiArray = new int[11][11];
	
	
public static void main(String[] args) {
	
	for (int i = 0; i <= 10; i++) {
		for (int j = 0; j<=10;j++) {
			int x;
			x = i*j;
			multiArray[i][j] = x;
		}
	}
	for (int i = 0; i < 10; i++) {
		for (int j = 0; j<10;j++) {
			if (multiArray[i+1][j+1] == 1) {
				System.out.print("\t");
			}
	System.out.print(multiArray[i+1][j+1]+ "\t" );
	}System.out.print("\n\t");
	}
}
}
